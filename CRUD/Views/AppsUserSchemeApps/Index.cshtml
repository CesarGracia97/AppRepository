@model IEnumerable<CRUD.Models.AppsUserSchemeApp>

@{
    ViewData["Title"] = "Apps/Usr/Esq";
}

<h1>Relacion Usr/Esq/App</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Create Relacion</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAppsSchemeApps)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAppsUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAppsSchemeApps)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAppsUser)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.Id">Editar</a> |
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.Id">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
